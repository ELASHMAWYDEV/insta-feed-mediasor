  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "edit-json-file": "^1.5.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.12",
    "nodemon": "^2.0.5",
    "puppeteer": "^5.4.1",
    "socket.io": "^2.3.0"
  }
}
